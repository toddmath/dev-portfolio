{
	"extends": "./.svelte-kit/tsconfig.json",
	"include": ["./src/**/*.d.ts", "./src/**/*.js", "./src/**/*.ts", "./src/**/*.svelte"],
	"compilerOptions": {
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$assets": ["src/assets"],
			"$assets/*": ["src/assets/*"],
			"$store": ["src/store"],
			"$store/*": ["src/store/*"],
			"$components": ["src/components"],
			"$components/*": ["src/components/*"]
		}
	}
}
